{
  "server": {
    "caching": {
      "enabled": true,
      "size": 32768,
      "ttl": 3600
    },
    "enabled": true,
    "logging": {
      "file": "/var/log/nginx/access.log",
      "format": "json",
      "level": "info"
    },
    "max_clients": 1024,
    "name": "nginx_proxy",
    "port": 80,
    "security": {
      "ssl_ciphers": ["ECDHE-RSA-AES256-GCM-SHA384", "AES256-SHA256"],
      "ssl_enabled": true,
      "ssl_protocols": ["TLSv1.2", "TLSv1.3"]
    },
    "upstream_servers": ["10.0.1.1:8080", "10.0.1.2:8080", "10.0.1.3:8080"],
    "version": "1.18.0"
  }
}